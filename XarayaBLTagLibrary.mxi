                <documenttype id="Xaraya" internaltype="Dynamic" winfileextension="xd,xt" macfileextension="xd,xt" file="default.xt" writebyteordermark="false" servermodel="PHP MySQL" previewfile="NewDocuments/default.xt">
                    <TITLE><![CDATA[Xaraya Template]]></TITLE>
                    <description><![CDATA[Xaraya Blocklayout document]]></description>
                </documenttype>
            </documenttype-insert>
        </documenttype-changes>
        <taglibrary-changes>
            <taglibrary-insert>
